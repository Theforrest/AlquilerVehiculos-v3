<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="600.0" spacing="10.0" stylesheets="@../estilos/VentanaBuscar.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.alquilervehiculos.vista.grafica.controladores.VentanaBuscarAlquiler">
   <children>
      <Label text="Cliente" />
      <Label fx:id="lbCliente" styleClass="muestra-texto" />
      <Label text="VEHICULO" />
      <Label fx:id="lbVehiculo" styleClass="muestra-texto" />
      <Label text="FECHA ALQUILER" />
      <Label fx:id="lbFechaAlquiler" styleClass="muestra-texto" />
      <Label text="FECHA DEVOLUCION" />
      <Label fx:id="lbFechaDevolucion" styleClass="muestra-texto" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btModificar1" mnemonicParsing="false" onAction="#borrar" text="Borrar" />
            <Button fx:id="btCerrar" mnemonicParsing="false" onAction="#cerrar" text="Cerrar" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
