<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="600.0" spacing="10.0" stylesheets="@../estilos/VentanaFormularios.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.alquilervehiculos.vista.grafica.controladores.VentanaDevolverAlquiler">
   <children>
      <ChoiceBox fx:id="cbTipoDevolucion" prefWidth="150.0" />
      <Label text="DNI" />
      <TextField fx:id="tfDni" />
      <Label text="Matricula" />
      <TextField fx:id="tfMatricula" />
      <Label text="FECHA DEVOLUCION" />
      <DatePicker fx:id="dpFechaDevolucion" />
      <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="30.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btDevolver" mnemonicParsing="false" onAction="#devolver" text="Devolver" />
            <Button fx:id="btCerrar" mnemonicParsing="false" onAction="#cerrar" text="Cerrar" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
