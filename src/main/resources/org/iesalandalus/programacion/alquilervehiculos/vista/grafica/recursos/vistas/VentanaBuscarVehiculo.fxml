<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="720.0" prefWidth="1280.0" spacing="10.0" stylesheets="@../estilos/VentanaBuscar.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.alquilervehiculos.vista.grafica.controladores.VentanaBuscarVehiculo">
   <children>
      <Label text="MATRICULA" />
      <Label fx:id="lbMatricula" styleClass="muestra-texto" />
      <Label text="MARCA" />
      <Label fx:id="lbMarca" styleClass="muestra-texto" />
      <Label text="MODELO" />
      <Label fx:id="lbModelo" styleClass="muestra-texto" />
      <Label text="CILINDRADA" />
      <Label fx:id="lbCilindrada" styleClass="muestra-texto" />
      <Label text="PLAZAS" />
      <Label fx:id="lbPlazas" styleClass="muestra-texto" />
      <Label text="PMA" />
      <Label fx:id="lbPma" styleClass="muestra-texto" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btModificar1" mnemonicParsing="false" onAction="#borrar" text="Borrar" />
            <Button fx:id="btModificar2" mnemonicParsing="false" onAction="#alquilar" text="Alquilar" />
            <Button fx:id="btDevolver" mnemonicParsing="false" onAction="#devolver" text="Devolver" />
            <Button fx:id="btCerrar" mnemonicParsing="false" onAction="#cerrar" text="Cerrar" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <TableView fx:id="tvAlquileres" prefHeight="400.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="tcCliente" minWidth="426.0" prefWidth="426.0" text="Cliente" />
            <TableColumn fx:id="tcFechaAlquiler" minWidth="426.0" prefWidth="426.0" text="Fecha Alquiler" />
            <TableColumn fx:id="tcFechaDevolucion" minWidth="426.0" prefWidth="426.0" text="Fecha Devolucion" />
        </columns>
      </TableView>
   </children>
</VBox>
