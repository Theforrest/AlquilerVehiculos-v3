<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0" stylesheets="@../estilos/VentanaInsertarCliente.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iesalandalus.programacion.alquilervehiculos.vista.grafica.controladores.VentanaInsertarAutobus">
   <children>
      <Label text="MATRICULA" />
      <TextField fx:id="tfMatricula" />
      <Label text="MARCA" />
      <TextField fx:id="tfMarca" />
      <Label text="MODELO" />
      <TextField fx:id="tfModelo" />
      <Label text="PLAZAS" />
      <TextField fx:id="tfPlazas" />
      <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="30.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btInsertar" mnemonicParsing="false" onAction="#insertar" text="Insertar" />
            <Button fx:id="btCerrar" mnemonicParsing="false" onAction="#cerrar" text="Cerrar" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
